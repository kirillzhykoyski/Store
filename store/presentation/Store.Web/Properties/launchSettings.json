{
  "iisSettings": {
    "windowsAuthentication": false, // Отключение аутентификации Windows
    "anonymousAuthentication": true, // Включение анонимной аутентификации
    "iisExpress": {
      "applicationUrl": "http://localhost:64083", // URL-адрес приложения для IIS Express
      "sslPort": 44305 // Порт для SSL
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress", // Использование IIS Express для запуска приложения
      "launchBrowser": true, // Автоматическое открытие браузера при запуске
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Установка переменной среды
      }
    },
    "Store.Web": {
      "commandName": "Project", // Запуск приложения через проект
      "launchBrowser": true, // Автоматическое открытие браузера при запуске
      "applicationUrl": "https://localhost:5001;http://localhost:5000", // URL-адреса, на которых будет запущено приложение
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
